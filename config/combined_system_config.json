  "domain_experts": {
    "enabled": ["mechanical", "electrical", "programming"],
    "save_prompts": true,
    "save_analyses": true
  },
  "document_generation": {
    "enabled_types": ["pdf", "diagram", "presentation", "document", "code"],
    "output_directory": "./data",
    "cleanup_temp_files": true
  }
}