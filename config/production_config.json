{
  "system_name": "Production Meta System",
  "version": "1.0.0",
  "description": "Combines Meta Model architecture with attempt2 document generation using only llama3.2",
  
  "llm_config": {
    "model": "llama3.2",
    "base_url": "http://localhost:11434",
    "timeout": 60,
    "temperature": 0.7,
    "max_retries": 3
  },
  
  "domain_experts": [
    "mechanical",
    "electrical", 
    "programming"
  ],
  
  "document_agents": [
    {
      "id": 1,
      "name": "PDF Report Generator",
      "type": "pdf",
      "description": "Generates comprehensive PDF reports using reportlab"
    },
    {
      "id": 2,
      "name": "Pipeline Diagram Generator", 
      "type": "diagram",
      "description": "Creates pipeline/flowchart diagrams using matplotlib"
    },
    {
      "id": 3,
      "name": "PowerPoint Presentation Generator",
      "type": "powerpoint", 
      "description": "Creates PowerPoint presentations using python-pptx"
    },
    {
      "id": 4,
      "name": "Word Document Generator",
      "type": "word",
      "description": "Generates Word documents using python-docx"
    },
    {
      "id": 5,
      "name": "Complex Code Project Generator",
      "type": "project",
      "description": "Creates complete code projects with multiple files"
    }
  ],
  
  "data_directory": "./data",
  "logging_level": "INFO",
  
  "validation": {
    "require_llama32": true,
    "no_mock_implementations": true,
    "strict_mode": true
  }
}